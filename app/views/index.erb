

<% @tasks.each do |task| %>
<div class="row">
  <div class="col s12 m6 offset-m3">
    <div class="card cyan">
      <div class="card-content white-text">
        <span class="card-title"><a class="white-text" href="tasks/<%= task.id %>">Task: <%= task.title %></a></span>
        <p>Description: <%= task.description %></p>
      </div>
    </div>
  </div>
</div>



<% end %>
